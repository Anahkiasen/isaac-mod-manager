{
  "name": "anahkiasen/isaac-mod-manager",
  "description": "A mod manager for Binding of Isaac: Afterbirth+",
  "license": "MIT",
  "keywords": [
    "isaac",
    "mods"
  ],
  "authors": [
    {
      "name": "Maxime Fabre",
      "email": "ehtnam6@gmail.com",
      "homepage": "http://about.me/anahkiasen"
    }
  ],
  "require": {
    "cache/filesystem-adapter": "^0.3.3",
    "cache/simple-cache-bridge": "^0.1.0",
    "illuminate/support": "^5.4",
    "league/container": "^2.4",
    "league/flysystem": "^1.0",
    "symfony/console": "^3.2",
    "symfony/process": "^3.2"
  },
  "require-dev": {
    "escapestudios/symfony2-coding-standard": "^2.10",
    "herrera-io/box": "^1.6",
    "kherge/box": "^2.7",
    "madewithlove/php-cs-fixer-config": "^1.2",
    "phpunit/phpunit": "^6.0",
    "symfony/var-dumper": "^3.2"
  },
  "autoload": {
    "psr-4": {
      "Isaac\\": "src"
    }
  },
  "bin": [
    "bin/imm"
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "scripts": {
    "build": "box build -vvv",
    "check-style": "phpcs",
    "fix-style": ["php-cs-fixer fix", "phpcbf"],
    "test": "phpunit"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  }
}
